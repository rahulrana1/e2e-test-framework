{
	"name": "acuit-testcafe",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test:chrome": "npx testcafe chrome tests/nexthub/excelImport.spec.js",
		"test:chrome:debugmode": "node -r esm runners/testrunner_local.js",
		"test:chrome:localParallel": "./shellScripts/executeTestCafeParallel_local.sh",
		"test:safari": "npx testcafe safari tests/nexthub/*.spec.js",
		"test:chrome:parallel": "./shellScripts/executeTestCafeParallel.sh",
		"test:chrome:parallel:videos": "./shellScripts/executeTestCafeParallel_Videos.sh",
		"data:cleanup": "node -r esm APITesting/jenkinsCleanUpRunner.js",
		"storeAdvisorLoginCookies": "node -r esm APITesting/advisorLoginAndStoreSession.js",
		"docker:build": "docker build -t testcafeimage -f Docker/dockerfile .",
		"docker:run": "docker run -i testcafeimage",
		"test:docker": "docker-compose up",
		"write:envfile": "env | grep -e ENVIRONMENT -e APPLICATION > allure/allure-results/environment.properties",
		"generate:allure:report": "allure generate allure/allure-results --clean -o allure/allure-report",
		"open:allure:report": "allure open allure/allure-report",
		"junit:merge": "npx junit-merge -d reports -o reports/junit_consolidated.xml",
		"slack:report:txtfile": " node utils/slack/slackMessage.js",
		"test:visual:local": "npx backstop test --config=visualTest/config/backstop_config.js",
		"test:visual:docker": "./shellScripts/executeVisualTestsDocker.sh",
		"test:railsNodejs:parallel": "mocha --parallel APITesting/tests/*.test.js",
		"test:rails": "mocha APITesting/tests/railsNextHub.test.js",
		"test:nodejs": "mocha APITesting/tests/nodejsNextHub.test.js"
	},
	"author": "Rahul",
	"license": "ISC",
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.0.20",
		"backstopjs": "^5.0.4",
		"chai": "^4.2.0",
		"csv-parser": "2.3.3",
		"dotenv": "^4.0.0",
		"esm": "^3.2.25",
		"find-config": "^1.0.0",
		"junit-merge": "^2.0.0",
		"mocha": "^8.1.3",
		"newman": "^5.2.0",
		"newman-reporter-html": "^1.0.3",
		"newman-reporter-htmlextra": "^1.19.4",
		"testcafe": "^1.9.4",
		"testcafe-reporter-allure": "^1.0.5",
		"testcafe-reporter-junit": "3.0.0"
	},
	"devDependencies": {
		"browserstack": "^1.5.3",
		"glob": "^7.1.4",
		"glob-promise": "^3.4.0",
		"testcafe-browser-provider-browserstack": "^1.9.0",
		"zlib": "^1.0.5",
		"recursive-readdir": "2.2.1",
		"shelljs": "^0.8.3",
		"expect.js": "0.3.1"
	}
}
